<div class="user-profile">
  <div class="page-title">
    <h1>User profile</h1>
  </div>

  <h5>Name: <%= @user.username %></h5>

  <%= link_to "Edit Profile", edit_user_path(@user) %><br />

  <%= link_to "Delete Account", user_path(@user), method: :delete, :confirm => "Are you sure you want to delete your account?" %>

  <h5>My Bucket List</h5>
  <%= link_to "Create Bucket List", new_user_my_places_list_path(@user) %>

  <h5> Visited Places List</h5>
  <%= link_to "Create Visited Places List", new_user_my_places_list_path(@user) %>
</div>
